# PostgreSQL Database Configuration
PGDATABASE=n8n
PGUSER=n8n
PGPASSWORD=your-secure-password-here
PGHOST=postgres
PGPORT=5432

# n8n Security
ENCRYPTION_KEY=your-very-long-encryption-key-at-least-32-characters

# n8n Configuration
N8N_HOST=localhost
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/

# n8n Package/Module Access Configuration
# Allow external npm packages (axios, lodash, moment, etc.)
NODE_FUNCTION_ALLOW_EXTERNAL=*
# Allow all built-in Node.js modules (fs, path, crypto, https, etc.)
NODE_FUNCTION_ALLOW_BUILTIN=*

# Optional: If you want to use a custom domain
# N8N_HOST=your-domain.com
# N8N_PROTOCOL=https
# WEBHOOK_URL=https://your-domain.com/ 